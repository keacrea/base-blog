{
    "name": "keacrea/base-blog",
    "description": "Blog plugin for keacrea",
    "type": "cakephp-plugin",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/keacrea/base-blog"
        }
    ],
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Blog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blog\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
